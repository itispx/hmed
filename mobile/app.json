{
  "expo": {
    "name": "HMED",
    "slug": "HMED",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#011826"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.hmed",
      "supportsTablet": true
    },
    "android": {
      "package": "com.hmed",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#011826"
      }
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#1F8A70"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "ed4bd3b8-fc65-4efb-a93e-29b86cf537ab"
      }
    }
  }
}
